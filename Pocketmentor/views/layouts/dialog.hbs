    {{#if flash}}
        <script>
            $(document).ready(function () {
                notify('top', 'right', '', '{{flash.type}}', 'animated fadeIn', 'animated fadeOut', '{{flash.title}}', '{{flash.message}}');

                if ('{{flash.goto}}' === 'register') {
                    $('#l-login').removeClass('toggled');
                    setTimeout(function () {
                        $('#l-register').addClass('toggled');
                    });
                }
            });
        </script>
    {{/if}}
